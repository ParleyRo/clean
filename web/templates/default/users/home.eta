<div>Server: <%= it.message %></div>

<div id="app">
	<app message="<%= it.message %>" :user-data="<%= it.data %>"></app>
</div>


<script type="module">

	import { createApp } from 'vue';
	
	import App from "/assets/default/js/components/user/App.js";

	const app = createApp({components: {App}});

	app.mount('#app');

</script>